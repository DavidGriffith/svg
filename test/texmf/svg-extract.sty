\renewcommand*\svg@extract[2]{%
  \ifnum\svgx@mode>\z@\relax%
    \expandafter\svg@get@path\expandafter[#2]{#1}%
    \if@svg@file@found%
      \begingroup%
        \def\svgx@file@out{\svg@file@path\svgx@file@name}%
        \endlinechar=\m@ne%
        \def\svg@read@line{}%
        \immediate\openout\svg@write@out=\svgx@file@out.tex%
        \immediate\openin\svg@read@in=\svgx@preamble%
        \@tempswatrue%
        \@svg@write@dotrue%
        \@whilesw\if@tempswa\fi{%
          \immediate\read\svg@read@in to\svg@read@line%
          \ifx\svg@read@line\@empty%
            \ifeof\svg@read@in\@tempswafalse\fi%
          \else%
            \svg@write@till{\svghidepreamblestart}{}%
            \svg@write@from{\svghidepreambleend}{}%
            \svg@write@till{\svgx@endpreamble}{\@tempswafalse}%
            \if@svg@write@do%
              \if@svgx@classfound\else%
                \expandafter\svgx@documentclass%
                  \svg@read@line\documentclass\documentclass\@nil%
              \fi%
              \ifx\svg@read@line\@empty\else%
                \immediate\write\svg@write@out{%
                  \unexpanded\expandafter{\svg@read@line}%
                }%
              \fi%
            \fi%
          \fi%
        }%
        \immediate\closein\svg@read@in%
        \let\svg@tempa\@empty%
        \def\svg@tempb##1{%
          \ifx\svg@tempa\@empty%
            \def\svg@tempa{##1}%
          \else%
            \expandafter\def\expandafter\svg@tempa\expandafter{%
              \svg@tempa,##1%
            }%
          \fi%
        }
        \ifdim\svg@param@width>\z@\relax%
          \svg@tempb{width=\svg@param@width}%
        \fi%
        \ifdim\svg@param@height>\z@\relax%
          \svg@tempb{height=\svg@param@height}%
        \fi%
        \ifdim\dimexpr\svg@param@scale\p@\relax=\p@\relax\else%
          \svg@tempb{scale=\svg@param@scale}%
        \fi%
        \ifx\svg@param@pretex\@empty\else%
          \svg@tempb{pretex=\unexpanded\expandafter{\svg@param@pretex}}%
        \fi%
        \ifx\svg@param@apptex\@empty\else%
          \svg@tempb{apptex=\unexpanded\expandafter{\svg@param@apptex}}%
        \fi%
        \edef\svg@tempa{\svg@tempa}%
        \immediate\write\svg@write@out{%
          \string\begin{document}^^J%
          \string\svgsetup{inkscape=false,extract=false}\@percentchar^^J%
          \ifx\svg@file@path\@empty\else%
            \string\svgsetup{svgpath=\svg@file@path}\@percentchar^^J%
          \fi%
          \if@svg@inkscape@latex\else%
            \string\svgsetup{inkscapelatex=false}%
          \fi%
          \ifx\svg@tempa\@empty\else%
            \string\svgsetup{\unexpanded\expandafter{\svg@tempa}}\@percentchar^^J%
          \fi%
          \string\newincludesvg{#1}\@percentchar^^J%
          \string\end{document}%
        }%
        \immediate\closeout\svg@write@out%
        % TODO PassOptionsToClass und Lualatex-kram
        \if@svgx@classfound\else%
          \immediate\openin\svg@read@in=\svgx@file@out.tex%
          \def\svg@tempa{}%
          \loop\unless\ifeof\svg@read@in%
            \readline\svg@read@in to\svg@read@line%
            \ifx\svg@read@line\@empty\else%
              \edef\svg@tempa{%
                \unexpanded\expandafter{\svg@tempa}%
                \unexpanded\expandafter{\svg@read@line}^^J%
              }%
            \fi%
          \repeat%
          \closein\svg@read@in%
          \immediate\openout\svg@write@out=\svgx@file@out.tex%
          \immediate\write\svg@write@out{\noexpand\documentclass{standalone}}%
          \immediate\write\svg@write@out{\unexpanded\expandafter{\svg@tempa}}%
          \closeout\svg@write@out%
        \fi%
        \ShellEscape{\svgx@latex\space\svgx@quote\svgx@file@out.tex\svgx@quote}%
        \ShellEscape{\svgx@latex\space\svgx@quote\svgx@file@out.tex\svgx@quote}%
        \ifx\svgx@dvips\@empty\else%
        \ShellEscape{\svgx@dvips\space\svgx@quote\svgx@file@name.dvi\svgx@quote}%
        \fi%
        \ifx\svgx@pstoeps\@empty\else%
        \ShellEscape{\svgx@pstoeps\space\svgx@quote\svgx@file@name.ps\svgx@quote}%
        \fi%
        \ifx\svgx@pstopdf\@empty\else%
        \ShellEscape{\svgx@pstopdf\space\svgx@quote\svgx@file@name.ps\svgx@quote}%
        \fi%
        \ifx\svgx@pdftops\@empty\else%
        \ShellEscape{\svgx@pdftops\space\svgx@quote\svgx@file@name.pdf\svgx@quote}%
        \fi%
        \ShellEscape{\svgx@mkdir\space\svgx@quote\svgx@file@path\svgx@quote}%
        \ShellEscape{%
          \svgx@mv\space\svgx@quote\svgx@file@name.*\svgx@quote\space%
          \svgx@quote\svgx@file@path\svgx@quote%
        }%
      \endgroup%
    \else%
    % TODO: Error
    \fi%
    \typeout{***** \svgx@mode}
  \fi%
%TODO: Test, ob zu erzeugende Datei existiert, sonst warnung
}


\newif\if@svgx@classfound
\newcommand*\svgx@documentclass{}
\def\svgx@documentclass#1\documentclass#2\documentclass#3\@nil{%
  \IfArgIsEmpty{#2}{}{%
    \def\svg@tempa{\@ifnextchar[{\svg@tempb}{\svg@tempb[]}}%]
    \def\svg@tempb[##1]##2\@nil{%
      \IfArgIsEmpty{##1}{%
        \def\svg@read@line{\documentclass[class=##2]{standalone}}%
      }{%
        \def\svg@read@line{\documentclass[class=##2,##1]{standalone}}%
      }%
    }%
    \svg@tempa#2\@nil%
    \@svgx@classfoundtrue%
  }%
}
\newcommand*\svg@write@skip{}
\def\svg@write@skip#1\@nil#2#3{%
  \def\svg@tempa##1{%
    \def\svg@tempa####1##1####2##1####3\@nil{%
      \IfArgIsEmpty{####3}{}{%
        \ifstr{#2}{till}{%
          \IfArgIsEmpty{####1}{}{%
            \immediate\write\svg@write@out{####1}%
          }%
          \@svg@write@dofalse%
        }{%
          \ifstr{#2}{from}{%
            \IfArgIsEmpty{####2}{%
              \def\svg@read@line{}%
            }{%
              \def\svg@read@line{####2}%
            }%
            \@svg@write@dotrue%
          }{}%
        }%
        #3%
      }%
    }%
  }%
  \edef\svg@tempb{\expandafter\detokenize\expandafter{#1}}%
  \expandafter\svg@tempa\expandafter{\svg@tempb}%
  \edef\svg@tempb{%
    \expandafter\detokenize\expandafter{\svg@read@line}\svg@tempb\svg@tempb%
  }%
  \expandafter\svg@tempa\svg@tempb\@nil%
}
\newcommand*\svg@write@till[2]{%
  \svg@write@skip#1\@nil{till}{#2}%
}
\newcommand*\svg@write@from[2]{%
  \svg@write@skip#1\@nil{from}{#2}%
}

