    \edef\svg@tempa{width=\svgwidth}
    \FamilyOptions{newSVG}{\svg@tempa}%
    \@tempswatrue%
  \fi%
  \if@tempswa%
    \PackageWarning{svg}{%
      You should specify the image size by using the\MessageBreak%
      appropriate parameters (`width' and `height' or `scale').\MessageBreak%
      Nevertheless, the value of%
      \ifx\svgwidth\@undefined%
        `\string\svgscale'%
      \else%
        `\string\svgwidth'%
      \fi%
      \space will be considered\MessageBreak
      as `\svg@tempa'%
    }%
    \let\svgwidth\@undefined%
    \let\svgscale\@undefined%
  \fi%
