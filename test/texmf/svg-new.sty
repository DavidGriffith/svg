\newcommand*\svg@get@path[2][.svg]{%
  \begingroup%
    \@svg@file@foundfalse%
    \let\svg@tempa\svg@file@path%
    \svg@get@@path{#2}{#1}%
    \ifx\svg@tempa\svg@file@path%
      \ifx\svg@path\@undefined\else%
